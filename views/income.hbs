 <div class="gh"> 
   <H2 style="margin-left:500px;">Shashika Traders Income</H2>

<a href="http://localhost:3000/addincome" class="btn btn-dark btn-lg text-primary" tabindex="-1" role="button" aria-disabled="true" >Categorize Income</a>

<table class="table table-striped w-50 p-3 " align="center">
  <thead>
    <tr>
      <th scope="col" class="bg-dark text-primary">Year</th>
      <th scope="col"  class="bg-dark text-primary">Month</th>
      <th scope="col"  class="bg-dark text-primary">Sales Income</th>
      <th scope="col" class="bg-dark text-primary">Spare Parts Income</th>
      <th scope="col" class="bg-dark text-primary">Ads Income</th>
      <th scope="col" class="bg-dark text-primary">Action</th>
    </tr>
  </thead>
  <tbody>
     {{#each income}}
    <tr>
      <th scope="row">{{this.year}}</th>
      <td>{{this.month}}</td>
      <td>{{this.sales}}</td>
      <td>{{this.spareparts}}</td>
      <td>{{this.ads}}</td>
      <td>
        <a href="/income/editIncome/{{ this.id }}"><button class="btn btn-dark text-primary">View</button></a> 
        <a href="/income/deleteIncome/{{ this.id }}"><button class="btn btn-dark text-primary">Delete</button></td></a>
      </tr>
    {{/each}}
    </tr>
  </tbody>
</table>
