<div class = "container " style="margin-top: 100px; margin-left:500px;" >
    <div class = "row">
        <h2 style="margin-left:100px;"> View Income Statement</h2>
        <div class="col-md-2"></div>
        <div class="col-md-8" >
            
            {{#each incomedata}}
            <form class="form-horizontal"method="post" action="../updatestat/{{this.id}}">
                <div class="form-group">
                    <label for="inputEmail3" class="col-md-2 control-label">Year </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="Year" name="month" value ="{{this.year}}">
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputEmail3" class="col-md-2 control-label">Total Income</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="Lastt Name" name="income" value ="{{this.income}}">
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputEmail3" class="col-md-2 control-label">Total Expenses</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="Email" name="expenses" value ="{{this.expenses}}">
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputPassword3" class="col-md-2 control-label">Net profit</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="Profession" name="netprofit" value ="{{this.netprofit}}">
                    </div>
                </div>
                
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <button type="submit" class="btn btn-dark text-primary" id="adduser">Update</button>
                    </div>
                </div>  
            </form>
            {{/each}}
        </div>
        

        <div class="col-md-2"></div>
    </div>

